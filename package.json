{
  "name": "clone-git",
  "version": "1.0.0",
  "description": "git clone cli alias.",
  "main": "app/dist/cloneGit.js",
  "types": "app/dist/cloneGit.d.ts",
  "scripts": {
    "build": "del-cli app/dist && concurrently \"npm run buildESM\" \"npm run buildCJS\" --raw",
    "buildESM": "tsc -p ./tsconfig.prod.esm.json && mjsify app/dist/esm",
    "buildCJS": "tsc -p ./tsconfig.prod.cjs.json",
    "dev": "npm run devNode",
    "devWeb": "rollup --config rollup.web.dev.config.js -w",
    "devNode": "rollup --config rollup.node.dev.config.js -w",
    "deploy": "npm run build && npm publish",
    "test": "node ./repl/dist/cloneGit-repl.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximilianMairinger/cloneGit.git"
  },
  "keywords": [
    "clone",
    "git"
  ],
  "bin": {
    "clone": "./app/dist/cloneGit.js",
    "cloneGit": "./app/dist/cloneGit.js"
  },
  "author": "Maximilian Mairinger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maximilianMairinger/cloneGit/issues"
  },
  "homepage": "https://github.com/maximilianMairinger/cloneGit#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.0",
    "@types/node": "^13.13.4",
    "@types/yargs": "^15.0.5",
    "concurrently": "^8.0.1",
    "del-cli": "^5.0.0",
    "mjsify": "^1.1.5",
    "rollup": "^2.18.1",
    "tslib": "2.0.0",
    "typescript": "^4.0.2",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "deepmerge": "^4.2.2",
    "fast-copy": "^2.1.0",
    "fast-equals": "^2.0.0",
    "inquirer": "^7.3.3",
    "josm-interpolate-string": "^1.1.1",
    "shelljs": "^0.8.4",
    "xrray": "^4.7.0",
    "yargs": "^16.0.3"
  }
}
